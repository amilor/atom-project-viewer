{
  "name": "project-viewer",
  "main": "./src/_index",
  "version": "0.3.18",
  "description": "Project viewer/manager that lets you add, edit and remove clients/groups/projects as well as switching between them.",
  "keywords": [
    "project",
    "manager",
    "viewer"
  ],
  "repository": "https://github.com/jccguimaraes/atom-project-viewer",
  "license": "MIT",
  "engines": {
    "atom": ">=1.9.6"
  },
  "dependencies": {
    "atom-space-pen-views": "latest"
  },
  "devDependencies": {
    "all-contributors-cli": "latest",
    "chai": "latest",
    "cz-conventional-changelog": "1.1.5",
    "loophole": "latest",
    "mocha": "latest"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "providedServices": {
    "project-viewer": {
      "description": "Access to creation of clients, groups and projects",
      "versions": {
        "1.0.0": "consumeProjectViewer"
      }
    }
  },
  "atomTestRunner": "./custom-test-runner"
}
